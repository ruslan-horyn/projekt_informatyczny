{
  "name": "projekt_informatyczny-server",
  "version": "1.0.0",
  "description": "server for project of IT",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "build": "tsc -p tsconfig.build.json",
    "server": "nodemon --watch src --ext ts --exec npm run start",
    "test": "jest --unhandled-rejections=strict",
    "test:watch": "test --watch",
    "test:ci": "npm run test -- --coverage --ci",
    "lint": "eslint \"src/**/*.{ts,js}\" --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "coverage-fix": "node jest-coverage-fixer.js"
  },
  "author": "Ruslan Horyn",
  "license": "",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^15.0.0",
    "envalid": "^7.3.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.9",
    "reflect-metadata": "^0.1.13",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
